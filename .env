# Bot Configuration
BOT_TOKEN=your_bot_token_here
API_ID=your_api_id_here
API_HASH=your_api_hash_here

# Logging
LOG_CHANNEL_ID=your_log_channel_id_here

# Modules Directory
MODULES_DIR=modules

# Permission System (comma-separated user IDs)
# Dev - Highest permission level
DEV_USERS=123456789,987654321

# Sudo - Mid permission level
SUDO_USERS=111111111,222222222

# Support - Lowest permission level
SUPPORT_USERS=333333333,444444444

# Database (optional, for future use)
DATABASE_URL=sqlite:///bot.db

# Other configurations
DEBUG=False
